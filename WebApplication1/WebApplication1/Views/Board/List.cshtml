@model List<WebApplication1.Models.Document>


@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>List</title>
</head>
<body>
    <table>
        <thead>
            <th>번호</th>
            <th>제목</th>
            <th>글쓴이</th>
        </thead>
        <tbody>
            @foreach(var item in Model)
            {
                <tr>
                    <td>@item.Document_Number</td>
                    <td>@item.Title</td>
                    <td>@item.Writer</td>
                </tr>
            }
        </tbody>
    </table>
</body>
</html>
